package com.swiggy;

public class MagicalGameRunner 
{
    public static void main( String[] args )
    {
        Player player1 = new Player("Jack", 0, 5, 10);
        Player player2 = new Player("Akash", 0, 10, 5);
        
        Arena arena = new Arena(player1, player2);
        arena.startMatch();
    }
}
